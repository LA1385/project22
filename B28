          function f5(){
            var i;
            var sum=0;
            for(i=0;i<arguments.length;i+=1){document.write(arguments[i]+"<br><br>");
              sum= sum+arguments[i]; 
               }
               document.write(sum + "<br><br>");
              }
          f5(65,19,24,56,73,87);
